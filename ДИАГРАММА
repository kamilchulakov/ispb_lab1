Table point {
  id int [pk]
  x float
  y float
}

Table place {
  id int [pk]
  point_id int [ref: > point.id]
  place_type_id int [ref: > place_type.id]
}

Table place_type {
  id int [pk]
  name varchar
}

Table road {
  id int [pk]
  name varchar
}

Table road_points {
  id int [pk]
  road_id int [ref: > road.id]
  point_id int [ref: > point.id]
}

Table road_work {
  id int [pk]
  road_id int [ref: > road.id]
  start_point int [ref: > point.id]
  end_point int [ref: > point.id]
  start_time timestamp
  end_time timestamp
}

Table track {
  id int [pk]
  road_id int [ref: > road.id]
  start_point int [ref: > point.id]
  end_point int [ref: > point.id]
  created_time timestamp
}

Table route {
  id int [pk]
  start_place int [ref: > place.id]
  end_place int [ref: > place.id]
}

Table route_points {
  id int [pk]
  route_id int [ref: > route.id]
  point_id int [ref: > point.id]
}

Table car {
  id int [pk]
  brand varchar
  model varchar
  left_top_wheel int [ref: > wheel.id]
  right_top_wheel int [ref: > wheel.id]
  left_back_wheel int [ref: > wheel.id]
  right_back_wheel int [ref: > wheel.id]
}

Table car_position {
  id int [pk]
  car_id int [ref: > car.id]
  point_id int [ref: > point.id]
}

Table wheel {
  id int [pk]
  size int
  brand varchar
}